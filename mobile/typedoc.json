{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": [
    "**/node_modules/**",
    "**/__tests__/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/build/**",
    "**/dist/**"
  ],
  "theme": "default",
  "name": "SportConnect Mobile Documentation",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "includeVersion": true,
  "hideGenerator": true,
  "categorizeByGroup": true,
  "categoryOrder": ["Components", "Screens", "Services", "Utils", "*"],
  "searchInComments": true,
  "readme": "README.md",
  "watch": false,
  "validation": {
    "invalidLink": true,
    "notExported": true
  },
  "sort": ["source-order"],
  "gitRevision": "main",
  "compilerOptions": {
    "skipLibCheck": true
  }
}
